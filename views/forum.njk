{% extends "layout.njk" %}
{% block  content %}
<main class="container mt-3">

    <h1>{{ title }}</h1>

    {% for row in rows %}
        <section class="post">
            <p>{{ row.author }}</p>
            <h1>{{ row.title }}</h1>
            <p>{{ row.content }} </p>
            <p>Posted {{ row.createdAt }}</p>
        </section>
    {% endfor %}

</main>
{% endblock %}