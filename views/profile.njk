{% extends "layout.njk" %}
{% block content %}
<h1>Profile</h1>
<p>Username: {{ name }}</p>
    {% for row in rows %}
        <section class="post">
            <p>{{ row.author }}</p>
            <h1>{{ row.title }}</h1>
            <p>{{ row.content }} </p>
            <p>Posted {{ row.createdAt }}</p>
        </section>
    {% endfor %}
{% endblock %}